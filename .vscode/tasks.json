{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/DiskMenuCore.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish x86",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish -c release -r win7-x86"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}